{
  "expo": {
    "name": "Native bet platform",
    "slug": "native-bet-platform",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/splash/icon.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/splash/screen.png",
      "resizeMode": "contain",
      "backgroundColor": "#09090a"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/splash/icon.png",
        "backgroundColor": "#09090a"
      },
      "package": "com.provd.nativebetplatform"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-router"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "eas": {
        "projectId": "e37677c8-0fef-43c5-bd15-30e558209b5e"
      }
    },
    "owner": "provd"
  }
}
